{
  "name": "ascension",
  "productName": "Ascension",
  "version": "1.0.0",
  "description": "Champion Mastery Statistics Application",
  "license": "LGPL-3.0-or-later",
  "private": false,
  "author": {
    "name": "Joel Coddington",
    "url": "https://joelc.dev/"
  },
  "engines": {
    "node": ">=9.0.0",
    "npm": ">=5.0.0",
    "yarn": ">=1.0.0"
  },
  "main": "main.js",
  "scripts": {
    "prod": "cross-env NODE_ENV=production webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "build": "cross-env NODE_ENV=production webpack --config webpack.build.config.js --mode production",
    "package": "yarn run build",
    "postpackage": "electron-packager ./ --out=./release/mac --overwrite --ignore=^/release --platform=darwin --arch=x64 --prune=true --icon=src/assets/ascension.icns && electron-packager ./ --out=./release/win --overwrite --ignore=^/release --platform=win32 --arch=ia32 --prune=true --icon=src/assets/ascension.ico"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/styles": "^4.11.2",
    "axios": "^0.21.1",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-number-format": "^4.4.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "sass-loader": "^8.0.2",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "babili-webpack-plugin": "^0.1.2",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "electron": "^9.0.0",
    "electron-devtools-installer": "^3.0.0",
    "electron-packager": "^14.2.1",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "postcss-pxtorem": "^5.1.1",
    "style-loader": "^1.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
